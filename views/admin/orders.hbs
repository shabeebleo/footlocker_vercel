
    <div id="right-panel" class="right-panel">
        <div class="breadcrumbs">
            <div class="breadcrumbs-inner">
                <div class="row m-0">
                    <div class="col-sm-4">
                        <div class="page-header float-left">
                            <div class="page-title">
                                <h1>Dashboard</h1>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-8">
                        <div class="page-header float-right">
                            <div class="page-title">
                                <ol class="breadcrumb text-right">
                                    <li><a href="#">Dashboard</a></li>
                                    <li><a href="#">Table</a></li>
                                    <li class="active">Basic table</li>
                                </ol>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="content">
            <div class="animated fadeIn">
                <div class="row">
                    <div class="">
                        <div class="card">
                            <div class="card-header">
                                <strong class="card-title">Order List</strong>
                            </div>
                            <div class="table-stats order-table ov-h">
                                <table class="table ">
                                    <thead>
                                        <tr>
                                            <th class="serial">#</th>

                                            <th>Delivery Date</th>
                                            <th>Order ID</th>
                                            <th>User ID</th>
                                            <th>Mobile </th>

                                            <th>Address </th>
                                            <th>Payment</th>
                                            <th>Amount</th>
                                            <th>Status</th>
                                            <th>Products</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        {{#each orders}}
                                        <tr>
                                            <td class="serial"></td>
                                            <td> {{isoToDate this.date}} </td>
                                            <td> <span class="product">{{this._id}}</span> </td>
                                            <td> <span class="name">{{this.userId}}</span> </td>
                                            <td> <span class="name">{{this.deliveryDetails.Mobile}}</span> </td>
                                            <td> <span>{{this.deliveryDetails.BuidlingName}},{{this.deliveryDetails.State}},{{this.deliveryDetails.District}},Pincode:{{this.deliveryDetails.Pincode}}</span>
                                            </td>
                                            <td> <span>{{this.Paymentmethod}}</span> </td>
                                         
                                            <td> <span>{{this.totalAmount}}</span> </td>
                                            <td> <span>{{this.status}}</span> </td>
                                            <td><a href="/admin/viewOrderedProducts/{{this._id}}" class="btn btn-info">view Products</a></td>
                                        </tr>
                                        {{/each}}
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
